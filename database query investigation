SELECT
	log.id, action, crud, component, courseid, eventname, userid, username, firstname, lastname, mdl_course.fullname, log.contextid, mdl_context.contextlevel

FROM
	mdl_logstore_standard_log AS log
INNER JOIN
	mdl_user ON log.userid = mdl_user.id
INNER JOIN
	mdl_course ON log.courseid = mdl_course.id
INNER JOIN
	mdl_context ON log.contextid = mdl_context.id
WHERE
	crud = 'c'
	AND
	eventname = '\\core\\event\\course_module_created'
ORDER BY log.id DESC
LIMIT 500;

-----

